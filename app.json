{
  "expo": {
    "name": "location",
    "slug": "location",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#242f3e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#242f3e"
      },
      "package": "com.iamtheluiz.location",
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_API"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
